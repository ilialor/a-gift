{
    "version": 2,
    "builds": [
      {
        "src": "app/vercel_test.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.12",
          "handler": "app:app",
          "buildCommand": "pip install -r requirements.txt",
          "installCommand": "pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/vercel_test.py"
      }
    ],
    "functions": {
      "app/vercel_test.py": {
        "memory": 1024,
        "maxDuration": 60
      }
    }
  }
