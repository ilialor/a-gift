{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.12",
          "handler": "app:app",
          "buildCommand": "pip install -r requirements.txt",
          "installCommand": "pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ],
    "env": {
      "PYTHONPATH": ".",
      "MAX_WORKERS": "1"
    }
  }
